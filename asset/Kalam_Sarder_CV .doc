<style>
	.spanJobPos
	{
		background-color: #e0e0e0;
		color: #333333;
		border-radius: 2px;
		padding: 4px 8px;
		font-size: 12px; 
		font-weight: normal;
		margin-bottom: 10px;
	}
	.ForHeaderDivAlign{
		 
		width: 100%;
	     		
	}   
	.BDJGrayArial11px
	{
		padding-left:5px; 
		margin:10px auto 10px auto;
		width:100%;/*600px;*/ 
	}

	.box {  
		width:100%;/*600px;*/  
		margin:10px auto 10px auto; 
		display:table; 
	}  
	.box .box-row {  
		display:table-row;  
	}  
	.box .box-cell {  
		display:table-cell;  
		/*padding:10px;  */
	}  
	.box .box-cell.box1 {     
		vertical-align:top;
		width:458px; 
	}  
	.box .box-cell.box2 {  
		width:16px;
	}  
	.box .box-cell.box3 {  
		width:126px; 
		vertical-align:middle;
		
	} 
	
	.box .box-cell.box_new1 {  
		vertical-align:top;
		width:600px;
		margin:15px auto 15px auto;		
	}  
		
	.box .box-cell.box_new2 {  
		vertical-align:top;
		width:600px;
		margin:15px auto 15px auto;		
	}

	.BDJHeadline01 {
		background: #f5f5f5;
		font-size: 15px;
		font-weight: bold;
		padding-top: 3px;
		padding-left: 2px;
		padding-bottom: 2px;
		line-height: 24px;    
		margin: 10px auto 10px auto;
		width: 100%; /*600px*/
	}

	.BDJNormalText01 {
		font-size: 14px;
		font-weight: normal;
		padding-top: 7px;
		padding-bottom: 10px;
		padding-left: 5px;
		margin: 10px auto 10px auto;
		width: 100%; /*600px*/
	}      
	.blue-cv-wrapper {
    color: #000000;
    padding: 30px 35px
	}

	.blue-cv-wrapper .download-list {
		margin-bottom: 15px;
		display: inline-flex;
		align-items: center
	}

	.blue-cv-wrapper .download-list .title {
		float: left;
		line-height: 24px;
		font-size: 16px;
		margin: 0 7px 0 0
	}

	.blue-cv-wrapper .download-list ul {
		margin: 0;
		padding: 0;
		float: left
	}

	.blue-cv-wrapper .download-list ul li {
		float: left;
		list-style: none;
		margin: 0 5px;
		display: inline
	}

	.blue-cv-wrapper .download-list ul li a {
		text-decoration: none;
		display: inline-block
	}

	.blue-cv-wrapper .download-list ul li a i {
		font-size: 20px;
		color: #1976d2
	}

	.blue-cv-wrapper .btn-l-tab-wrap {
		float: right
	}

	.blue-cv-wrapper .btn-l-tab-wrap .s-txt {
		color: #146AFF;
		margin-right: 10px
	}

	.blue-cv-wrapper .btn-l-tab-wrap .btn,
	.blue-cv-wrapper .btn-l-tab-wrap .btn:hover,
	.blue-cv-wrapper .btn-l-tab-wrap .btn:focus {
		box-shadow: none;
		background-color: #ffffff;
		border: 1px solid #1976D2;
		color: #1976D2;
		font-size: 13px;
		padding: 4px 8px;
		font-weight: bold
	}

	.blue-cv-wrapper .btn-l-tab-wrap .btn.active {
		background-color: #1976D2;
		color: #ffffff
	}

	table.blue-cv {
		border-spacing: 0;
		border-collapse: collapse;
		width: 100%;
		border: 1px solid #DCDCDC;
		font-family: open sans, SolaimanLipi
	}

	table.blue-cv .left-side {
		background-color: #F5F5F5;
		padding: 20px 30px 20px 30px;
		width: 42%;
		vertical-align: top
	}

	table.blue-cv .right-side {
		padding: 25px 0 25px 40px;
		width: 58%;
		vertical-align: top
	}

	table.t-l-pinfo {
		border-spacing: 0;
		border-collapse: collapse;
		width: 100%;
		table-layout: auto
	}

	table.t-l-pinfo h3 {
		margin-top: 10px;
		line-height: 1.1
	}

	table.t-l-pinfo td.t-a-c img {
		max-width: 124px;
		height: 131px;
		table-layout: auto
	}

	table.t-l-pinfo td.t-n-a-info-wrap {
		margin: 0 0 0 0;
		padding: 20px 0 15px 0
	}

	table.t-n-a-info {
		border-spacing: 0;
		border-collapse: collapse;
		width: 100%;
		font-size: 14px
	}

	table.t-n-a-info img {
		margin-right: 5px
	}

	table.t-n-a-info td,
	table.t-n-a-info a {
		color: #000000;
		font-weight: 600;
		text-decoration: none
	}

	table.t-n-a-info a {
		word-break: break-word
	}

	table.t-n-a-info td {
		padding-bottom: 10px;
		vertical-align: text-top
	}

	table.t-l-pinfo td.t-per-info-wrap {
		margin: 0 0 0 0;
		padding: 10px 0 10px 0
	}

	table.t-per-info {
		border-spacing: 0;
		border-collapse: collapse;
		width: 100%
	}

	table.t-per-info td {
		display: table-cell
	}

	table.t-per-info img {
		margin-right: 10px
	}

	table.t-per-info td.t-title {
		color: #2F64A3;
		font-size: 18px
	}

	table.t-per-info td.p-info-dtl {
		margin: 0 0 0 0;
		padding: 15px 0 0 0
	}

	table.t-per-info td.p-info-dtl .s-txt {
		color: #494949;
		margin-bottom: 3px
	}

	table.t-per-info td.p-info-dtl .d-txt {
		font-weight: 600;
		color: #000000
	}

	table.e-s-t-e-r {
		border-spacing: 0;
		border-collapse: collapse;
		width: 100%
	}

	table.e-s-t-e-r td.exp-info-wrap {
		padding: 15px 0 10px 0;
		border-bottom: 1px solid #CBCBCB
	}

	table.e-s-t-e-r td.exp-info-wrap-s {
		padding: 25px 0 10px 0;
		border-bottom: 1px solid #CBCBCB
	}

	table.e-s-t-e-r td.exp-info-wrap-t {
		padding: 25px 0 10px 0;
		border-bottom: 1px solid #CBCBCB
	}

	table.e-s-t-e-r td.exp-info-wrap-ed {
		padding: 25px 0 10px 0;
		border-bottom: 1px solid #CBCBCB
	}

	table.e-s-t-e-r td.exp-info-wrap-r {
		padding: 25px 0 10px 0
	}

	table.e-t-e-r-info {
		border-spacing: 0;
		border-collapse: collapse;
		width: 100%;
		font-size: 14px
	}

	table.e-t-e-r-info td {
		display: table-cell
	}

	table.e-t-e-r-info img {
		margin-right: 10px
	}

	table.e-t-e-r-info td.t-title {
		color: #2F64A3;
		font-size: 18px
	}

	table.e-t-e-r-info td.t-title .t-exp {
		font-weight: bold;
		font-size: 16px
	}

	table.e-t-e-r-info td.p-info-dtl {
		margin: 0 0 0 0;
		padding: 0 0 15px 0
	}

	table.e-t-e-r-info td.p-info-dtl .p-title {
		color: #000000;
		font-size: 14px;
		font-weight: bold;
		margin-bottom: 10px
	}

	table.e-t-e-r-info td.p-info-dtl .s-txt {
		color: #494949;
		margin-bottom: 3px
	}

	table.e-t-e-r-info td.p-info-dtl .d-txt {
		color: #000000;
		font-weight: 600
	}

	table.e-t-e-r-info td.p-info-dtl.sk-name-wrap {}

	table.e-t-e-r-info td.p-info-dtl .sk-name {
		font-size: 13px;
		padding: 8px 10px;
		border: 1px solid #CBCBCB;
		border-radius: 4px;
		display: inline-block;
		margin-right: 5px;
		margin-top: 3px;
		margin-bottom: 3px
	}

	@media only screen and (max-width:767px) {
		table.blue-cv {
			border: none
		}

